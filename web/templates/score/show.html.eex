<h2>Show score</h2>

<ul>

  <li>
    <strong>Talent:</strong>
    <%= @score.talent_id %>
  </li>

  <li>
    <strong>Job:</strong>
    <%= @score.job_id %>
  </li>

  <li>
    <strong>Score:</strong>
    <%= @score.score %>
  </li>

</ul>

<%= link "Edit", to: score_path(@conn, :edit, @score) %>
<%= link "Back", to: score_path(@conn, :index) %>
